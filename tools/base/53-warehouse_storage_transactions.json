"CREATE TABLE IF NOT EXISTS `warehouse_storage_transactions` (`id` int(11) NOT NULL AUTO_INCREMENT,`warehouse_id_from` smallint(5) DEFAULT NULL,`warehouse_id_in` smallint(5) DEFAULT NULL,`item_id` int(11) DEFAULT NULL,`item_name` varchar(500) DEFAULT NULL,`item_manufacturer` char(255) DEFAULT NULL,`item_qty` int(11) DEFAULT NULL,`item_price` decimal(65,1) DEFAULT 0.0,`responsible_id` int(11) DEFAULT NULL,`cost` decimal(65,1) DEFAULT 0.0,`price_cash` decimal(65,1) DEFAULT 0.0,`price_card` decimal(65,1) DEFAULT 0.0,`price_transfer` decimal(65,1) DEFAULT 0.0,`is_moving` tinyint(1) DEFAULT NULL,`is_written_off` tinyint(1) DEFAULT NULL,`is_sold` tinyint(1) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"