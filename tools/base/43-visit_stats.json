"CREATE TABLE IF NOT EXISTS `visit_stats` (`id` int(11) NOT NULL AUTO_INCREMENT,`visit_id` int(11) DEFAULT NULL,`parent_id` int(11) DEFAULT NULL,`stat` tinyint(1) DEFAULT NULL,`pressure` char(20) DEFAULT NULL,`pulse` smallint(5) DEFAULT NULL,`temperature` float DEFAULT NULL,`saturation` tinyint(4) DEFAULT NULL,`breath` float DEFAULT NULL,`urine` float DEFAULT NULL,`description` varchar(700) DEFAULT NULL,`add_date` datetime NOT NULL DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"