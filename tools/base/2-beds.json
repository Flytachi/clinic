"CREATE TABLE IF NOT EXISTS `beds` (`id` smallint(5) NOT NULL AUTO_INCREMENT,`branch_id` smallint(5) DEFAULT NULL,`building_id` tinyint(2) DEFAULT NULL,`ward_id` smallint(5) DEFAULT NULL,`type_id` tinyint(3) DEFAULT NULL,`building` char(200) DEFAULT NULL,`floor` tinyint(3) DEFAULT NULL,`ward` smallint(6) DEFAULT NULL,`bed` smallint(5) NOT NULL,`types` char(50) DEFAULT NULL,`client_id` int(11) DEFAULT NULL,`add_date` datetime NOT NULL DEFAULT current_timestamp(),PRIMARY KEY (`id`),UNIQUE KEY `building_id` (`building_id`,`floor`,`ward_id`,`bed`) USING BTREE) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"