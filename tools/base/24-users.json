"CREATE TABLE IF NOT EXISTS `users` (`id` int(11) NOT NULL AUTO_INCREMENT,`parent_id` int(11) DEFAULT NULL,`username` char(20) DEFAULT NULL,`password` char(70) DEFAULT NULL,`is_active` tinyint(1) DEFAULT NULL,`first_name` char(30) DEFAULT NULL,`last_name` char(30) DEFAULT NULL,`father_name` char(30) DEFAULT NULL,`birth_date` date DEFAULT NULL,`province_id` tinyint(3) DEFAULT NULL,`province` char(255) DEFAULT NULL,`region_id` smallint(5) DEFAULT NULL,`region` char(255) DEFAULT NULL,`passport_seria` char(50) DEFAULT NULL,`passport_pin_fl` char(50) DEFAULT NULL,`work_place` char(255) DEFAULT NULL,`work_position` char(255) DEFAULT NULL,`phone_number` char(50) DEFAULT NULL,`address_residence` varchar(500) DEFAULT NULL,`address_registration` varchar(500) DEFAULT NULL,`gender` tinyint(1) DEFAULT NULL,`user_level` tinyint(4) NOT NULL,`division_id` smallint(5) DEFAULT NULL,`status` tinyint(1) DEFAULT 1,`share` float DEFAULT NULL,`is_foreigner` tinyint(1) DEFAULT NULL,`room_id` int(11) DEFAULT NULL,`token_telegramm` char(250) DEFAULT NULL,`pacs_login` char(50) DEFAULT NULL,`pacs_password` char(50) DEFAULT NULL,`insurance_policy` char(100) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`),UNIQUE KEY `username` (`username`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"