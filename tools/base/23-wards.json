"CREATE TABLE IF NOT EXISTS `wards` (`id` smallint(5) NOT NULL AUTO_INCREMENT,`branch_id` smallint(5) DEFAULT NULL,`building_id` tinyint(2) DEFAULT NULL,`division_id` smallint(5) DEFAULT NULL,`floor` tinyint(3) DEFAULT NULL,`ward` smallint(6) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`),UNIQUE KEY `building_id` (`building_id`,`floor`,`ward`) USING BTREE) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"