"CREATE TABLE IF NOT EXISTS `package_bypass` (`id` int(11) NOT NULL AUTO_INCREMENT,`is_active` tinyint(1) DEFAULT 1,`autor_id` int(11) DEFAULT NULL,`name` char(255) DEFAULT NULL,`items` longtext DEFAULT NULL,`method` tinyint(2) DEFAULT NULL,`description` varchar(500) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"