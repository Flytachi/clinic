"CREATE TABLE IF NOT EXISTS `templates` (`id` mediumint(7) NOT NULL AUTO_INCREMENT,`autor_id` int(11) NOT NULL,`name` varchar(500) NOT NULL,`description` text DEFAULT NULL,`add_date` datetime NOT NULL DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"