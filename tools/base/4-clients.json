"CREATE TABLE IF NOT EXISTS `clients` (`id` int(11) NOT NULL AUTO_INCREMENT,`responsible_id` int(11) DEFAULT NULL,`status` tinyint(1) DEFAULT NULL,`gender` tinyint(1) DEFAULT NULL,`is_foreigner` tinyint(1) DEFAULT NULL,`first_name` char(30) DEFAULT NULL,`last_name` char(30) DEFAULT NULL,`father_name` char(30) DEFAULT NULL,`birth_date` date DEFAULT NULL,`province_id` tinyint(3) DEFAULT NULL,`province` char(255) DEFAULT NULL,`region_id` smallint(5) DEFAULT NULL,`region` char(255) DEFAULT NULL,`passport_seria` char(50) DEFAULT NULL,`passport_pin_fl` char(50) DEFAULT NULL,`work_place` char(255) DEFAULT NULL,`work_position` char(255) DEFAULT NULL,`phone_number` char(50) DEFAULT NULL,`address_residence` varchar(500) DEFAULT NULL,`address_registration` varchar(500) DEFAULT NULL,`token_telegramm` char(250) DEFAULT NULL,`insurance_policy` char(100) DEFAULT NULL,`add_date` datetime(6) DEFAULT current_timestamp(6),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"