"CREATE TABLE IF NOT EXISTS `visit_documents` (`id` int(11) NOT NULL AUTO_INCREMENT,`visit_id` int(11) DEFAULT NULL,`parent_id` int(11) DEFAULT NULL,`file_format` char(100) DEFAULT NULL,`file_size` bigint(20) DEFAULT NULL,`file_extension` char(255) DEFAULT NULL,`location` varchar(500) DEFAULT NULL,`mark` char(100) DEFAULT NULL,`description` varchar(755) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"