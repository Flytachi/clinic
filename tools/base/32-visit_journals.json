"CREATE TABLE IF NOT EXISTS `visit_journals` (`id` mediumint(7) NOT NULL AUTO_INCREMENT,`visit_id` int(11) NOT NULL,`responsible_id` int(11) NOT NULL,`record` text NOT NULL,`last_update` datetime DEFAULT NULL,`add_date` datetime NOT NULL DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"