"CREATE TABLE IF NOT EXISTS `queue` (`id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,`room_id` int(11) DEFAULT NULL,`user_id` int(11) DEFAULT NULL,`is_queue` tinyint(1) DEFAULT 1,`is_accept` tinyint(1) DEFAULT NULL,`is_delete` tinyint(1) DEFAULT NULL,PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"