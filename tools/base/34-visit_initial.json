"CREATE TABLE IF NOT EXISTS `visit_initial` (`id` int(11) NOT NULL AUTO_INCREMENT,`visit_id` int(11) DEFAULT NULL,`responsible_id` int(11) DEFAULT NULL,`weight` char(255) DEFAULT NULL,`height` char(255) DEFAULT NULL,`temperature` char(255) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"