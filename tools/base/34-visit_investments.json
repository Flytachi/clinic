"CREATE TABLE IF NOT EXISTS `visit_investments` (`id` int(11) NOT NULL AUTO_INCREMENT,`visit_id` int(11) DEFAULT NULL,`user_id` int(11) DEFAULT NULL,`pricer_id` int(11) DEFAULT NULL,`balance_cash` decimal(65,1) NOT NULL DEFAULT 0.0,`balance_card` decimal(65,1) NOT NULL DEFAULT 0.0,`balance_transfer` decimal(65,1) NOT NULL DEFAULT 0.0,`expense` tinyint(1) DEFAULT NULL,`add_date` datetime NOT NULL DEFAULT current_timestamp(),`expense_date` datetime DEFAULT NULL,PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"