"CREATE TABLE IF NOT EXISTS `visit_operations` (`id` int(11) NOT NULL AUTO_INCREMENT,`branch_id` smallint(5) DEFAULT NULL,`visit_id` int(11) NOT NULL,`client_id` int(11) NOT NULL,`grant_id` int(11) DEFAULT NULL,`division_id` smallint(5) DEFAULT NULL,`operation_id` smallint(5) DEFAULT NULL,`operation_name` varchar(500) DEFAULT NULL,`operation_cost` decimal(65,1) DEFAULT 0.0,`operation_date` date DEFAULT NULL,`operation_time` time DEFAULT NULL,`operation_end_date` date DEFAULT NULL,`operation_end_time` time DEFAULT NULL,`completed` tinyint(1) DEFAULT NULL,`completed_date` datetime DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"