"CREATE TABLE IF NOT EXISTS `users` (`id` int(11) NOT NULL AUTO_INCREMENT,`branch_id` smallint(5) DEFAULT NULL,`username` char(20) DEFAULT NULL,`password` char(70) DEFAULT NULL,`is_active` tinyint(1) DEFAULT NULL,`first_name` char(30) DEFAULT NULL,`last_name` char(30) DEFAULT NULL,`father_name` char(30) DEFAULT NULL,`user_level` tinyint(4) NOT NULL,`division_id` smallint(5) DEFAULT NULL,`pacs_login` char(50) DEFAULT NULL,`pacs_password` char(50) DEFAULT NULL,`add_date` datetime DEFAULT current_timestamp(),PRIMARY KEY (`id`),UNIQUE KEY `username` (`username`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;"